{
    "name": "teamones/phpcent",
    "description":"PHP library to communicate with Centrifugo HTTP API（use workerman support async request）",
    "keywords": ["centrifugo", "websocket", "realtime", "messaging"],
    "homepage": "https://github.com/centrifugal/phpcent",
    "version": "3.1.1",
    "license": "MIT",
    "require":{
        "php": ">=5.4.0",
        "ext-json":"*",
        "ext-curl":"*",
        "workerman/http-client": "^0.1.7"
    },
    "require-dev": {
        "phpunit/phpunit": "5.*"
    },
    "autoload": {
        "psr-4": { "phpcent\\": "src" }
    }
}
